<%= form_for @subhouse do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :house_id %><br />
    <%= f.number_field :house_id %>
  </p>
  <p>
    <%= f.label :solar %><br />
    <%= f.number_field :solar %>
  </p>
  <p>
    <%= f.label :melange %><br />
    <%= f.text_field :melange %>
  </p>
  <p>
    <%= f.label :material %><br />
    <%= f.text_field :material %>
  </p>
  <p>
    <%= f.label :exp %><br />
    <%= f.number_field :exp %>
  </p>
  <p><%= f.submit %></p>
<% end %>
